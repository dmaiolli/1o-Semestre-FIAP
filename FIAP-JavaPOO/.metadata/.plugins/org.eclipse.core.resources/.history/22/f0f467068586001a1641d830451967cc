package ex03;

public class Voo {

	private int numero;
	private Data data;
	private boolean[] lugaresOcupados;

	public Voo(int numero, Data data) {
		this.numero = numero;
		this.data = data;
		this.lugaresOcupados = new boolean[100];
	}
	public int getNumero() {
		return numero;
	}
	
	public Data getData() {
		return data;
	}

}
